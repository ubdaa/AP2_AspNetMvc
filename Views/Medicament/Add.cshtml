@model MedicamentViewModel

@{
    ViewBag.Title = "Ajout d'un médicament";
    Layout = "_Layout";
}

<h2>Ajout d'un médicament</h2>

<form asp-action="Add" method="post" class="w-full flex flex-col gap-4">

    @Html.Partial("_Form", Model)

    <input class="btn btn-primary my-5" type="submit" value="Ajouter le médicament"/>
</form>